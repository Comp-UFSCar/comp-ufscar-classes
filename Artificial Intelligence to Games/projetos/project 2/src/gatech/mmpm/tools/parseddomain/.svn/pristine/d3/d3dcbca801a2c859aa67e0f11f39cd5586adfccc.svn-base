package gatech.mmpm.tools.parseddomain;

/**
 * Class that represents an exception while parsing the
 * XML with the domain.
 *  
 * @author Pedro Pablo Gomez Martin
 * @date August, 2009
 */
public class ParseException extends java.lang.Exception {

	/**
	 * UID for serialization purposes. 
	 */
	private static final long serialVersionUID = 235473703346398634L;

	/**
	 * Constructor
	 * 
	 * @param cause Cause of the exception.
	 */
	public ParseException(String cause) {
		super(cause);
	}
	
} // class ParseException
